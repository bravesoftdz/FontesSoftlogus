unit MargemLucro;

interface

uses
  Windows, Messages, SysUtils, Variants, Classes, Graphics, Controls, Forms,
  Dialogs, StdCtrls, FnpNumericEdit, dxGDIPlusClasses, Vcl.ExtCtrls, Vcl.Mask, RxToolEdit,
  RxCurrEdit;

type
  TFMargemLucro = class(TForm)
    Label1: TLabel;
    eitems: TCurrencyEdit;
    Image1: TImage;
    procedure FnpNumericEdit1KeyPress(Sender: TObject; var Key: Char);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  FMargemLucro: TFMargemLucro;

implementation

{$R *.dfm}

procedure TFMargemLucro.FnpNumericEdit1KeyPress(Sender: TObject;
  var Key: Char);
begin
  if key = #13 then
    Close;
end;

end.
