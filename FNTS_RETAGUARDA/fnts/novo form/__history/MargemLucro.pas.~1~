unit MargemLucro;

interface

uses
  Windows, Messages, SysUtils, Variants, Classes, Graphics, Controls, Forms,
  Dialogs, StdCtrls, FnpNumericEdit;

type
  TFMargemLucro = class(TForm)
    FnpNumericEdit1: TFnpNumericEdit;
    Label1: TLabel;
    procedure FnpNumericEdit1KeyPress(Sender: TObject; var Key: Char);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  FMargemLucro: TFMargemLucro;

implementation

{$R *.dfm}

procedure TFMargemLucro.FnpNumericEdit1KeyPress(Sender: TObject;
  var Key: Char);
begin
  if key = #13 then
    Close;
end;

end.
