  object qrDRE: TZQuery
    Connection = frmmodulo.econexao2
    OnCalcFields = qrDRECalcFields
    Active = True
    SQL.Strings = (
      'Select'
      ''
      '  Classificacao,'
      '  max(tipo) as Tipo,'
      '  max(Nome_Conta) as Nome_conta,'
      '  sum(Total) as Total'
      'from'
      '  ( Select'
      '      c000035.classificacao as Classificacao,'
      '      c000035.tipo as tipo,'
      '      c000035.conta as Nome_Conta,'
      '      sum(c000042.Valor) as Total'
      '    from'
      '      c000042, c000035'
      '    where'
      '      c000042.codconta = c000035.codigo'
      '    group by'
      '      Classificacao,'
      '      tipo,'
      '      Nome_Conta'
      '    UNION'
      '    Select'
      '      c000035.classificacao as Classificacao,'
      '      c000035.tipo as tipo,'
      '      c000035.conta as Nome_Conta,'
      '      sum(c000044.Valor) as Total'
      '    from'
      '      c000044, c000035'
      '    where'
      '      c000044.codconta = c000035.codigo'
      '    group by'
      '      Classificacao,'
      '      tipo,'
      '      Nome_Conta'
      '  )  as tmp'
      '  group by classificacao'
      'order by classificacao')
    Params = <>
    Left = 248
    Top = 168
    object qrDRECLASSIFICACAO: TStringField
      FieldName = 'CLASSIFICACAO'
      ReadOnly = True
      Size = 10
    end
    object qrDRETIPO: TIntegerField
      FieldName = 'TIPO'
      ReadOnly = True
    end
    object qrDRENOME_CONTA: TStringField
      FieldName = 'NOME_CONTA'
      ReadOnly = True
      Size = 40
    end
    object qrDRETOTAL: TFloatField
      FieldName = 'TOTAL'
      ReadOnly = True
    end
    object qrDREnome_tipo: TStringField
      FieldKind = fkCalculated
      FieldName = 'nome_tipo'
      Size = 50
      Calculated = True
    end
  end
